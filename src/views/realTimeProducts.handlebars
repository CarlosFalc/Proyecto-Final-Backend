<h4>Página principal</h4>
<div>
    <h2>Bienvenido</h2>
    <p>Listado de Servicios:</p>
    <ul>
            {{#each Services}}
            <li>Servicio: {{this.title}}  - Descripción: {{this.description}}  - Código: {{this.code}}  - Precio: {{this.price}}  - Stock: {{this.stock}} - Categoría: {{this.category}} - Imágen: {{this.thumbnail}} - Id: {{this.id}}</li><br>
            {{/each}}
    </ul>
</div>

<input type="text" id="chatBox" placeholder="servicio"/><br>
<input type="text" id="prd" placeholder="descripción"/><br>
<input type="number" id="cod" placeholder="código"/><br>
<input type="number" id="pr" placeholder="precio"/><br>
<input type="number" id="stk" placeholder="stock"/><br>
<input type="text" id="cat" placeholder="categoría"/><br>
<input type="text" id="link" placeholder="link de imágen"/><br>
<input type="nmuber" id="di" placeholder="Id"/><br>
<br><button id="sendButton">Agregar</button>
<button id="resetButton">Eliminar</button>

<div id="divServices"></div>

<script src="socket.io/socket.io.js"></script>
<script src="js/home.js"></script>
