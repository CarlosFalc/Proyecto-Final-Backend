<h4>Página principal</h4>
<script src="socket.io/socket.io.js"></script>
<script src="js/home.js"></script>
<div>
    <h2>Bienvenido</h2>
    <p>Listado de Servicios:</p>
    <ul>
            {{#each Services}}
            <li>Servicio: {{this.title}}  - Descripción: {{this.description}}  - Código: {{this.code}}  - Precio: {{this.price}}  - Stock: {{this.stock}} - Categoría: {{this.category}} - Imágen: {{this.thumbnail}} - Id: {{this.id}}</li><br>
            {{/each}}
    </ul>
    <div>
        <form id="formulario" class="form-inline">
        <div class="form-group mx-sm-3 mb-2">
          <input class="form-control" id="chatBox" type="text" id="texto" placeholder="title">
          <input class="form-control" id="chatBox" type="text" id="texto" placeholder="description">
          <input class="form-control" id="chatBox" type="number" id="texto" placeholder="Price">
          <input class="form-control" id="chatBox" type="text" id="texto" placeholder="thumbnail">
          <input class="form-control" id="chatBox" type="number" id="texto" placeholder="code">
          <input class="form-control" id="chatBox" type="number" id="texto" placeholder="stock">
          <input class="form-control" id="chatBox" type="text" id="texto" placeholder="status">
          <input class="form-control" id="chatBox" type="text" id="texto" placeholder="category">
          {{!-- <input class="form-control" id="chatBox" type="number" id="texto" placeholder="Id">         --}}
        </div>
            
        <button type="submit" class="btn btn-primary mb-2">Agregar</button>
        <button type="submit" class="btn btn-primary mb-2">Eliminar</button>
     </form>
    </div>
</div>